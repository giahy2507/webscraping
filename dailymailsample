            <div style="text-align: center;"><script type="text/javascript">
  (function (top) {
    if (top.document.getElementById('taboola-top-carousel-thumbnails')) return;

    colorThumbnails();
    insertWidget();

    function colorThumbnails() {
      if (window.getComputedStyle) {
        var selectedNav = top.document.querySelector('.nav-primary .link-wocc a');
        var selectedNavStyles = top.getComputedStyle(selectedNav, '');
        var bgColor = selectedNavStyles.getPropertyValue('background-color');
      } else {
        var bgColor = '#00AAD2';
      }

      var stylesheet = document.createElement('style');
      stylesheet.type = 'text/css';
      stylesheet.innerHTML =
        '.thumbnails-d .videoCube .video-label-box {' +
          'background-color: ' + bgColor + ';' +
        '}' +
        '.thumbnails-d .videoCube .video-label-box .video-title {' +
          'padding: 6px 6px 0px;' +
        '}' +
        '.thumbnails-d {' +
          'margin: 15px auto;' +
          'border-color: ' + bgColor + ';' +
        '}';

      top.document.body.appendChild(stylesheet);
    }

    function insertWidget() {
      var container = document.createElement('div');
      var floatingButtons = document.getElementsByClassName('floating-buttons')[0];

      if (!floatingButtons) return;

      container.addEventListener('click', getClickHandler());
      floatingButtons.parentNode.insertBefore(container, floatingButtons);
      top._taboola = top._taboola || [];
      top._taboola.push({
        mode: 'thumbnails-d',
        container: container,
        placement: 'Ad Blocked Above Article Thumbnails',
        target_type: 'mix'
      });
    }

    function getClickHandler() {
      return typeof ABE === 'object' && typeof ABE.clickTracker === 'function'
        ? ABE.clickTracker('taboola-billboard')
        : function () {};
    }
  }(window.top));
</script></div>
  
